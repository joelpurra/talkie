{
  "manifest_version": 2,
  "version": "4.1.0",
  "version_name": "v4.1.0 Free (2017-07-24)",
  "applications": {
    "gecko": {
      "strict_min_version": "52.0"
    }
  },
  "content_security_policy": "default-src 'self'; script-src 'self'; object-src 'self'; img-src 'self' data:;",
  "externally_connectable": {
    "ids": []
  },
  "name": "__MSG_extensionName__",
  "short_name": "__MSG_extensionShortName__",
  "description": "__MSG_extensionDescription_Free__",
  "author": "Joel Purra",
  "homepage_url": "https://joelpurra.com/projects/talkie/",
  "background": {
    "scripts": [
      "lib/browser-polyfill.min.js",
      "dist/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "optional_permissions": [
    "clipboardRead"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+A",
        "mac": "Command+Shift+A"
      },
      "description": "__MSG_commandBrowserActionDescription__"
    },
    "start-stop": {
      "suggested_key": {
        "default": "Alt+Shift+A"
      },
      "description": "__MSG_commandStartStopDescription__"
    },
    "read-clipboard": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "__MSG_commandReadClipboardDescription__",
      "global": true
    },
    "open-website-main": {
      "suggested_key": {
        "default": "Alt+Shift+0"
      },
      "description": "__MSG_commandOpenWebsiteMainDescription__"
    },
    "open-website-store-free": {
      "suggested_key": {
        "default": "Alt+Shift+9"
      },
      "description": "__MSG_commandOpenWebsiteStoreDescription_Free__"
    },
    "open-website-store-premium": {
      "suggested_key": {
        "default": "Alt+Shift+8"
      },
      "description": "__MSG_commandOpenWebsiteStoreDescription_Premium__"
    }
  },
  "options_ui": {
    "page": "src/options/options.html",
    "chrome_style": true
  },
  "requirements": {},
  "offline_enabled": true,
  "default_locale": "en",
  "browser_action": {
    "browser_style": false,
    "default_title": "__MSG_buttonDefaultTitle_Free__",
    "default_icon": {
      "16": "resources/icon/free/icon-play/icon-16x16.png",
      "32": "resources/icon/free/icon-play/icon-32x32.png",
      "48": "resources/icon/free/icon-play/icon-48x48.png",
      "64": "resources/icon/free/icon-play/icon-64x64.png",
      "128": "resources/icon/free/icon-play/icon-128x128.png",
      "256": "resources/icon/free/icon-play/icon-256x256.png",
      "512": "resources/icon/free/icon-play/icon-512x512.png",
      "1024": "resources/icon/free/icon-play/icon-1024x1024.png"
    }
  },
  "icons": {
    "16": "resources/icon/free/icon-play/icon-16x16.png",
    "32": "resources/icon/free/icon-play/icon-32x32.png",
    "48": "resources/icon/free/icon-play/icon-48x48.png",
    "64": "resources/icon/free/icon-play/icon-64x64.png",
    "128": "resources/icon/free/icon-play/icon-128x128.png",
    "256": "resources/icon/free/icon-play/icon-256x256.png",
    "512": "resources/icon/free/icon-play/icon-512x512.png",
    "1024": "resources/icon/free/icon-play/icon-1024x1024.png"
  }
}
